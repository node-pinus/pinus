{
  "name": "pinus-example-robot",
  "version": "1.2.18",
  "private": false,
  "description": "A distributed load test framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/node-pinus/pinus"
  },
  "scripts": {
    "build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node --max_old_space_size=4096 ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json **/*.ts",
		"fix-lint": "node --max_old_space_size=4096 ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json **/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-loader lib/"
  },
  "dependencies": {
    "@types/node": "^8.5.2",
    "@types/ws": "^3.2.0",
    "pinus-protobuf": "^1.2.14",
    "pinus-protocol": "^1.2.14",
    "pinus-robot": "^1.2.18",
    "typescript": "^2.6.2",
    "ws": "^3.2.0"
  }
}
