  },
  "dependencies": {
    "@types/node": "^20.11.24",
    "@types/ws": "^8.5.4",
    "cli-color": "^2.0.0",
    "pinus-protobuf": "^1.7.0",
    "pinus-protocol": "^1.7.0",
    "pinus-robot": "^1.7.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
