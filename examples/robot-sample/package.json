{
  "name": "pinus-example-robot",
  "version": "1.7.0",
  "private": false,
  "description": "A distributed load test framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/node-pinus/pinus"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && cd dist &&  node app",
