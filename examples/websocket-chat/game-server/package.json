        "bluebird": "^3.5.1",
        "crc": "^4.3.2",
        "pinus": "^1.7.1",
        "pinus-robot": "^1.7.1",
        "pinus-robot-plugin": "^1.7.1",
        "reflect-metadata": "^0.2.2",
        "source-map-support": "^0.5.0"
    },
    "devDependencies": {
        "tslint": "^6.1.3",
        "typescript": "^4.3.5"
    }
