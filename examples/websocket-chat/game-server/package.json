{
    "name": "pinus-sample-websocket-chat-game-server",
    "version": "1.7.1",
    "private": false,
    "main": "./dist/app",
    "scripts": {
        "build": "node_modules/.bin/tsc",
        "start": "yarn run build && cd dist &&  node app",
