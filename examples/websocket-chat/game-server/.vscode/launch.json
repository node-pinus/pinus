    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "LaunchMaster",
            "program": "${workspaceFolder}\\dist\\app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Connector",
            "address": "127.0.0.1",
            "port": 10001,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Chat",
            "address": "127.0.0.1",
            "port": 10002,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Gate",
            "address": "127.0.0.1",
            "port": 10003,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}"
        }

    ]
}