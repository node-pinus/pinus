{
  "name": "pinus",
  "version": "1.3.3",
  "homepage": "https://github.com/mybios/pinus",
  "repository": {
    "type": "git",
    "url": "https://github.com/mybios/pinus.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus lib/"
  },
  "bugs": {
    "url": "https://github.com/mybios/pinus/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mybios/pinus#license"
    }
  ],
  "keywords": [
    "pinus",
    "pinus",
    "framework",
    "game",
    "web",
    "realtime",
    "server"
  ],
  "files": [
    "bin",
    "dist",
    "lib",
    "template",
    "test"
  ],
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "@types/mocha": "^2.2.46",
    "@types/mqtt-connection": "git+https://github.com/node-pinus/types-mqtt-connection.git",
    "@types/socket.io": "^1.4.31",
    "async": "^2.6.0",
    "cliff": "^0.1.10",
    "commander": "^2.12.2",
    "crc": "^3.5.0",
    "eyes": "^0.1.8",
    "mkdirp": "^0.5.1",
    "mqtt-connection": "^3.2.0",
    "node-bignumber": "^1.2.1",
    "pinus-admin": "^1.3.0",
    "pinus-loader": "^1.3.0",
    "pinus-logger": "^1.3.0",
    "pinus-protobuf": "^1.3.0",
    "pinus-protocol": "^1.3.0",
    "pinus-rpc": "^1.3.2",
    "pinus-scheduler": "^1.3.0",
    "seq-queue": "^0.0.5",
    "socket.io": "^2.0.4",
    "ws": "^3.3.3"
  },
  "bin": {
    "pinus": "./dist/bin/pinus.js"
  },
  "devDependencies": {
    "@types/async": "^2.0.46",
    "@types/cliff": "^0.1.4",
    "@types/commander": "^2.12.2",
    "@types/crc": "^3.4.0",
    "@types/eyes": "^0.1.31",
    "@types/mkdirp": "^0.5.2",
    "@types/ws": "^3.2.1",
    "mocha": "^4.1.0",
    "muk": ">=0.0.1",
    "nyc": "^8.4.0",
    "should": "^3.3.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
