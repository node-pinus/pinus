{
  "name": "pinus",
  "version": "1.4.10",
  "homepage": "https://github.com/mybios/pinus",
  "repository": {
    "type": "git",
    "url": "https://github.com/mybios/pinus.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "mochatest": "cd dist && mocha --require source-map-support/register --recursive",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus lib/"
  },
  "engines": {
    "node": ">=8.5"
  },
  "bugs": {
    "url": "https://github.com/mybios/pinus/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mybios/pinus#license"
    }
  ],
  "keywords": [
    "pinus",
    "pinus",
    "framework",
    "game",
    "web",
    "realtime",
    "server"
  ],
  "files": [
    "bin",
    "dist",
    "lib",
    "template",
    "test"
  ],
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "@types/socket.io": "^2.1.6",
    "async": "^3.0.0",
    "cliff": "^0.1.10",
    "commander": "^3.0.0",
    "crc": "^3.5.0",
    "eyes": "^0.1.8",
    "mkdirp": "^0.5.1",
    "mqtt-connection": "^4.0.0",
    "node-bignumber": "^1.2.1",
    "pinus-admin": "^1.4.10",
    "pinus-loader": "^1.4.10",
    "pinus-logger": "^1.4.10",
    "pinus-protobuf": "^1.4.10",
    "pinus-protocol": "^1.4.10",
    "pinus-rpc": "^1.4.10",
    "pinus-scheduler": "^1.4.10",
    "seq-queue": "^0.0.5",
    "socket.io": "^2.3.0",
    "ws": "^7.0.0"
  },
  "bin": {
    "pinus": "./dist/bin/pinus.js"
  },
  "devDependencies": {
    "@types/async": "3.2.3",
    "@types/cliff": "0.1.5",
    "@types/commander": "2.12.2",
    "@types/crc": "3.4.0",
    "@types/eyes": "0.1.31",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.7",
    "@types/ws": "7.2.8",
    "mocha": "6.2.3",
    "muk": "0.5.3",
    "nyc": "15.1.0",
    "should": "13.2.3",
    "tslint": "5.20.1",
    "typescript": "4.0.5"
  }
}
