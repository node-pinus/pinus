{
  "name": "pinus-cli",
  "version": "1.2.9-3",
  "description": "pinus command line client for managing pinus",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ../../node_modules/typescript/bin/tsc",
    "start": "npm run build && node dist/index",
    "lint": "node ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "test": "npm run build",
    "cov": "nyc mocha",
    "ci": "npm run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-cli lib/"
  },
  "repository": "",
  "keywords": [
    "pinus",
    "command",
    "line",
    "maintening"
  ],
  "main": "./dist/index",
  "types": "./dist/index.d.ts",
  "bin": {
    "pinus-cli": "./dist/bin/pinus-cli.js"
  },
  "dependencies": {
    "@types/async": "^2.0.46",
    "@types/cliff": "^0.1.4",
    "@types/node": "^8.5.2",
    "@types/optimist": "0.0.29",
    "async": "^2.6.0",
    "cliff": "^0.1.10",
    "optimist": "^0.6.1",
    "pinus-admin": "^1.2.9-3",
    "pinus-logger": "^1.2.9-3"
  },
  "devDependencies": {
    "typescript": "^2.6.2"
  },
  "license": "MIT"
}
