{
    "name": "pinus-logger",
    "version": "1.2.8",
    "private": false,
    "main": "./dist/index",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "node ../../node_modules/typescript/bin/tsc",
        "start": "npm run build && node dist/index",
        "lint": "node ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
        "test": "npm run build",
        "cov": "nyc mocha",
        "ci": "npm run test",
        "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-logger lib/"
    },
    "dependencies": {
        "@types/node": "^8.5.2",
        "log4js": "^2.4.1"
    },
    "devDependencies": {
        "mocha": ">=0.0.1",
        "should": ">=0.0.1",
        "typescript": "^2.6.2"
    }
}
