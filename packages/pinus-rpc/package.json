{
	"name": "pinus-rpc",
	"version": "1.3.2",
	"main": "./dist/index",
	"types": "./lib/index",
	"files": [
		"dist",
		"lib",
		"test",
		"sample",
		"index.ts"
	],
	"scripts": {
		"build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts sample/**/*.ts",
		"fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts sample/**/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-rpc lib/"
	},
	"dependencies": {
		"@types/async": "^2.0.46",
		"@types/bluebird": "^3.5.19",
		"@types/crc": "^3.4.0",
		"@types/node": "^8.5.2",
		"@types/socket.io": "^1.4.31",
		"@types/socket.io-client": "^1.4.32",
		"@types/uuid": "^3.4.3",
		"@types/ws": "^3.2.1",
		"async": "^1.5.2",
		"bearcat-buffer": "^0.1.x",
		"bluebird": "^3.5.1",
		"crc": "^0.2.1",
		"mqtt-connection": "^3.2.0",
		"pinus-loader": "^1.3.0",
		"pinus-logger": "^1.3.0",
		"socket.io": "^2.0.4",
		"source-map-support": "^0.5.0",
		"stream-pkg": "0.0.5",
		"uuid": "^3.1.0",
		"ws": "^3.3.3"
	},
	"devDependencies": {
		"mocha": "^4.1.0",
		"should": "^13.2.0",
		"tslint": "^5.9.1",
		"typescript": "^2.9.2"
	}
}
