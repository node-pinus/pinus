{
  "name": "pinus-robot-plugin",
  "version": "1.2.19",
  "description": "pinus robot plugin",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node --max_old_space_size=4096 ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node --max_old_space_size=4096 ../../node_modules/tslint/bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-monitor lib/"
   
  },
  "main": "./dist/index",
  "types": "./src/index",
  "dependencies": {
    "@types/node": "^8.5.2",
    "@types/yargs": "^10.0.1",
    "pinus": "^1.2.19",
    "yargs": "^10.0.3"
  },
  "license": "MIT"
}
